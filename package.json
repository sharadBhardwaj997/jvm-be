    "express-validator": "^7.0.1",
    "lodash": "4.17.21",
    "shelljs": "0.8.5",
    "uuid": "^9.0.1",
    "yeoman-environment": "3.19.3",
    "yeoman-generator": "5.9.0"
  },
  "devDependencies": {
    "chai": "4.3.10",
    "fs-extra": "11.1.1",
    "mocha": "10.2.0",
    "sinon": "16.1.0",
    "yeoman-assert": "3.1.1",
    "yeoman-test": "6.3.0"
  }
